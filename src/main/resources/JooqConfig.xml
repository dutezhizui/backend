<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration>
    <logging>WARN</logging>
    <!--数据库配置信息-->
    <jdbc>
        <driver>com.mysql.jdbc.Driver</driver>
        <url>jdbc:mysql://123.57.241.67:3306/miniapp?useUnicode=true&amp;characterEncoding=utf8&amp;autoReconnect=true&amp;rewriteBatchedStatements=TRUE&amp;useSSL=false&amp;serverTimezone=UTC</url>
        <user>root</user>
        <password>123456</password>
    </jdbc>
    <generator>
        <name>org.jooq.codegen.JavaGenerator</name>
        <strategy>
            <!--<name>org.jooq.codegen.DefaultGeneratorStrategy</name>-->
            <name>com.darkcom.backend.config.MyStrategy</name>
        </strategy>
        <database>
            <name>org.jooq.meta.mysql.MySQLDatabase</name>
            <!--表名-->
            <includes>t_user_info</includes>
            <!--数据库名-->
            <inputSchema>miniapp</inputSchema>
            <outputSchemaToDefault>false</outputSchemaToDefault>
        </database>

        <target>
            <!--包名-->
            <packageName>com.darkcom.backend.generate</packageName>
            <directory>src/main/java</directory>
            <!--是否删除文件夹中已有文件-->
            <clean>false</clean>
        </target>
        <generate>
            <daos>true</daos>
            <pojos>true</pojos>
            <records>true</records>
            <globalObjectReferences>true</globalObjectReferences>
            <generatedAnnotation>false</generatedAnnotation>
            <indexes>false</indexes>
            <relations>false</relations>
            <tables>true</tables>
            <javaTimeTypes>true</javaTimeTypes>
        </generate>
    </generator>
</configuration>